   "source": [
    "primes = [2]\n",
    "for num in range(3, 100):\n",
    "    isPrime = True\n",
    "    for prime in primes:\n",
    "        if num % prime == 0:\n",
    "            isPrime = False\n",
    "            break\n",
    "    if isPrime:\n",
    "        primes.append(num)\n"
   ]
  },
